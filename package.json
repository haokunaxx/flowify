{
  "name": "flowify",
  "version": "0.1.0",
  "description": "Flowify - 工作流引擎和可视化编排器",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "dev": "pnpm -r --parallel run dev",
    "demo": "npx tsx examples/demo.ts",
    "demo:ui": "npx tsx examples/demo-ui.ts"
  },
  "keywords": [
    "workflow",
    "engine",
    "dag",
    "typescript",
    "monorepo"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "fast-check": "^3.15.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.1.0"
  }
}
